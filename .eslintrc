
{
  "root": true,
  "env": {
    "browser": true
  },
  "plugins": [],
  "parser": "babel-eslint",
  "extends": ["prettier"],
  "rules": {
    "object-curly-newline": "off",
    "no-unused-vars": ["warn"],
    "no-console": "warn",
    "no-trailing-spaces" : "error",
    "eol-last": 0,
    "max-len": ["error", { "code": 180 }],
  }
}

